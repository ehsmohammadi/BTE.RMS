
@{
    ViewBag.Title = "هدف فرعی جدید";
}

<h2 class="page-header">هدف فرعی جدید</h2>

<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-md-2">عنوان هدف فرعی</label>
        <div class="col-md-10">
            <input type="text" class="form-control"/>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">هدف کلی</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">اولویت</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">زمان هدف</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">شروع</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">پایان</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">ضرب العجل</label>
        <div class="col-md-10">
            <select class="form-control"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">شرح(ویژگی های هدف)</label>
        <div class="col-md-10">
            <input type="text" class="form-control"/>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">ماموریت(اقدامی که برای رسیدن به هدف باید انجام دهم)</label>
        <div class="col-md-10">
            <input type="text" class="form-control"/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="button" class="btn btn-default">ثبت</button>
        </div>
    </div>
</div>
<hr />
<p>
    @Html.ActionLink("برگشت به لیست", "SecTargetList")
</p>